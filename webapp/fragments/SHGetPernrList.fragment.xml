<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="mySelectDialog"
		noDataText="No Products Found"
		title="Select Product"
		search=".onSearch"
		confirm=".onDialogClose"
		cancel=".onDialogClose"
		items="{
			path: 'mainModel>/pernrList'
		}">
		<StandardListItem
			title="{mainModel>Sicilno}"
			description="{mainModel>Username} {mainModel>Usersname}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>